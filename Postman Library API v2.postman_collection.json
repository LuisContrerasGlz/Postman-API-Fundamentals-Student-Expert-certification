		{
			"name": "skill check",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Parse the response body as JSON",
							"let responseBodytest = pm.response.json();",
							"",
							"// Extract the actorName from the response body",
							"let actorName = responseBodytest.actorName;",
							"",
							"// Set the actorName as a collection variable named \"favoriteActor\"",
							"pm.collectionVariables.set(\"favoriteActor\", actorName);",
							"",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "apikey",
					"apikey": [
						{
							"key": "value",
							"value": "skillcheck",
							"type": "string"
						},
						{
